{
  "pluginName": "jsformat",
  "description": "格式化HTML，JS，CSS",
  "author": "zhangfeixiang",
  "homepage": "https://github.com/zhangfeixiang/utools-plugin-jsformat.git",
  "main": "index.html",
  "version": "1.0.0",
  "logo": "logo.png",
  "preload": "preload.js",
  "pluginSetting": {
    "single": true
  },
  "features": [
    {
      "code": "jsformat",
      "explain": "代码格式化",
      "cmds": [
        "js",
        "html",
        "css",
        "format",
        {
          "type": "files",
          "label": "代码格式化",
          "match": "/\\.(html|css|js)$/i"
        },
        {
          "type": "text",
          "match": "/^(.*)$/",
          "label": "js格式化"
        }
      ]
    }
  ]
}
